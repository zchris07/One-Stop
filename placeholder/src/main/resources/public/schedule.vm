<style>
    /* style when project is started */
    .started {
        color: white;
        font-weight: bold;
        background: green;
        padding: 5px;
        border-radius: 5px;
    }
    .nav-item{
        color: white;
    }
    /* style when project is completed */
    .completed {
        color: white;
        font-weight: bold;
        background: greenyellow;
        padding: 5px;
        border-radius: 5px;
    }
    .navbar-custom {
        background-color: darkblue;
        opacity: 0.75;
    }
    /* Modify brand and text color */

    .navbar-custom .navbar-brand,
    .navbar-custom .navbar-text {
        color: beige;
    }
    .btn-primary {
        color: midnightblue;
        background-color: lightskyblue;
        border-color: steelblue;
    }
    .btn-fail {
        color: midnightblue;
        background-color: slateblue;
        border-color: darkslateblue;
    }
    .btn-profile {
        background-image: url("https://www.pngfind.com/pngs/m/282-2828527_supermario-supermariomaker-mystery-mushroom-mariomushroom-8-bit-kirby.png");
        background-size: 70px;
        width: 70px;
        height: 70px;
    }
    .btn-circle.btn-xl {
        width: 70px;
        height: 70px;
        padding: 10px 16px;
        border-radius: 35px;
        font-size: 12px;
        text-align: center;
    }
    .gutter {
        margin-left: 10px;
        margin-right: 10px;
        margin-top: 25px;
    }
    .box{
        position: relative;
    }
    .box:after {
        content:'';
        background-color: whitesmoke;
        position: absolute;
        top:0px;
        left: 0px;
        width:100%;
        height:100%;
        z-index:-1;
        opacity: 0.7; /* Here is your opacity */
    }
    body {
        background-image: url('https://wallpaperaccess.com/full/513246.jpg');
        background-repeat: no-repeat;
        background-attachment: fixed;
        background-size: 100% 100%;
        opacity: 0.9;
    }
</style>

